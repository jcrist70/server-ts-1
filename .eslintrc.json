{
  "env": {
    "browser": true,
    "commonjs": true,
    // "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended", 
    "plugin:@typescript-eslint/recommended"
    // "airbnb",
    // "airbnb-typescript"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "project":"/Users/hannah/Downloads/CODE/PARALLEL/server-ts/tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": "warn",
    "eqeqeq": "warn",
    "@typescript-eslint/ban-types": ["error",
    {
        "types": {
            "String": false,
            "Boolean": false,
            "Number": false,
            "Symbol": false,
            "{}": false,
            "Object": false,
            "object": false,
            "Function": false
        },
        "extendDefaults": true
      }
    ]
  }
}
